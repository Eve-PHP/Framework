{
	"name": "eve-php/framework",
    "type": "framework",
    "description": "Framework Built on top of the Eden Library",
	"minimum-stability": "dev",
    "keywords": [
		"eve", 
		"framework"
	],
    "homepage": "http://eve.openovate.com",
    "license": "MIT",
	"authors": [
        {
            "name": "Christian Blanquera",
            "email": "cblanquera@openovate.com"
        }
    ],
	"bin": [
        "bin/eve"
    ],
	"require-dev": {
        "phpunit/phpunit": "~4.4",
        "squizlabs/php_codesniffer": "~1.5"
    },
    "require": {
		"php": ">=5.4.1",
        "eden/cookie": "4.*",
        "eden/folder": "4.*",
        "eden/handler": "4.*",
        "eden/handlebars": "4.*",
        "eden/image": "4.*",
        "eden/language": "4.*",
        "eden/mail": "4.*",
        "eden/mysql": "4.*",
        "eden/postgre": "4.*",
        "eden/sqlite": "4.*",
        "eden/oauth": "4.*",
        "eden/server": "4.*",
        "eden/session": "4.*",
        "eden/template": "4.*",
        "eden/timezone": "4.*",
        "eden/validation": "4.*",
		"ruflin/elastica": "2.3.0",
		"videlalvaro/php-amqplib": "2.5.2"
    },
	"autoload": {
        "psr-4": {
			"Eve\\Framework\\": "/src",
			"Eve\\": "/../../../"
		}
    }
}